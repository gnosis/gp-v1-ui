const {
  serializeError, getMessageFromCode,
} = __webpack_require__(165)
const ethErrors = __webpack_require__(399)
const ERROR_CODES = __webpack_require__(90)

module.exports = {
  ethErrors,
  EthereumRpcError,
  EthereumProviderError,
  serializeError,
  getMessageFromCode,
  /** @type ErrorCodes */
  ERROR_CODES,
